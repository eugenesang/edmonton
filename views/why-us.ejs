                    <button class="estimates">Start</button>
                </div>
            </section>

            <%- include('faqs') %>
                <%- include('estimates') %>
                    <%- include('footer') %>
    </body>

    <style>
        p {
            font-size: normal;
        }

        h2 {
            font-family: var(--dm);
        }

        .h-txt-container {
            background-color: transparent;
            margin: 20px auto;
        }

        header .header-left-container {
            margin-top: 20vh;
        }

        @media (max-width: 992px) {
            header .header-left-container {
                margin: 0px;
                padding-top: 90px;
                min-height: calc(100vh - 100px);
                display: grid;
                align-content: space-around;
                align-items: center;
            }
        }

        .hiw {
            display: grid;
            grid-template-rows: 1fr auto;
            gap: 1rem;
            max-width: 90vw;
            width: fit-content;
            margin: 25px auto;
            padding: 15px;
            background-color: rgba(203, 203, 203, 0.5);
            box-shadow: 1px 1px 5px 2px gray;
            border-radius: 5px;
        }

        .hiw-img {
            background: url(/images/woman-celebrating-successful-swift-sale.jpg);
            aspect-ratio: 3/2;
            background-size: contain !important;
            width: 300px;
            max-width: 90vw;
            margin: 5px auto;
            border-radius: 5px;
        }

        .hiw-img.e-1 {
            background-image: url(/images/pro-real-estate-guidance.jpg);
        }

        .hiw-img.e-2 {
            background: url(/images/pexels-andrea-piacquadio-3756681.jpg);
        }

        .hiw-img.e-3 {
            background: url(/images/pexels-max-rahubovskiy-6758779.jpg);
        }

        .hiw-img.e-4 {
            background: url(/images/pexels-kindel-media-7578905.jpg);
            aspect-ratio: 16/9;
        }

        .hiw-right {
            max-width: 80vw;
            margin: 0px auto;
        }

        .hiw h5 {
            font-family: var(--dm);
            font-size: x-large;
            padding: 10px 5px;
        }

        .hiw p {
            font-size: 1rem;
        }

        @media screen and (min-width: 768px) {
            .hiw {
                grid-template-columns: 350px auto;
            }

            .hiw p {
                font-size: 1.1rem;
            }
        }

        .m-h-header {
            width: fit-content;
            padding: 10px;
            margin: 5px auto;
        }

        .m-h-header h2,
        #requirements h2 {
            text-align: center;
            font-size: 2.4rem;
        }

        @media screen and (min-width: 1080px) {
            .hiw {
                max-width: 75vw;
                margin: 10px auto;
                border-radius: 5px;
            }

            .hiw p {
                font-size: 1.2rem;
            }
        }

        .req-list li::before {
            display: inline-block;
            content: "\2713";
            margin-right: 8px;
            color: var(--orange);
            font-weight: 600;
        }

        #requirements {
            padding: 10px 5vw;

        }

        .req-img {
            text-align: center;
        }

        .req-img img {
            width: 60vw;
            max-width: 500px;
            border-radius: 10px;
        }

        .req-body {
            width: fit-content;
            margin: 10px auto;
            margin-bottom: 1rem;
            border-radius: 4px;
            padding: 15px;
            box-shadow: 1px 1px 5px 2px #808080c9;
        }

        .req-list {
            width: fit-content;
            margin: 10px auto;
        }

        .req-list li {
            list-style: none;
            list-style-type: none;
            margin: 0;
            font-family: "adobe-garamond-pro", serif;
            transition: ease 400ms;
            display: flex;
            align-items: center;
            padding: .3rem;
        }

        @media screen and (min-width: 768px) {
            .req-list {
                display: grid;
                grid-template-columns: 1fr 1fr;
                gap: 1rem;
            }
        }
    </style>

</html>